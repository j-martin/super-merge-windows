{
  "manifest_version": 3,
  "name": "Super Merge Windows",
  "version": "2.0.0",
  "description": "Remove duplicates, sort and merge all tabs into the active window",
  "homepage_url": "https://github.com/j-martin/merge-windows",
  "minimum_chrome_version": "125",
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_title": "Merge Windows",
    "default_icon": "icon.png"
  },
  "permissions": ["tabs"]
}
